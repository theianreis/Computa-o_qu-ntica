   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = np.linspace(0, 1, 1000)\n",
    "f = np.exp(-x**2)\n",
    "area = np.trapz(f, x)\n",
    "print(f\"Área aproximada = {area}\")"
